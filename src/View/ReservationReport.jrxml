	<queryString language="SQL">
		<![CDATA[SELECT 
    g.guest_id,
    g.first_name,
    g.last_name,
    g.email,
    g.phone,
    r.reservation_no,
    r.room_no,
    r.check_in_date,
    r.check_out_date,
    r.nights,
    r.total_amount,
    r.payment_method
FROM 
    guest g
JOIN 
    reservation r ON g.guest_id = r.guest_id
WHERE 
    r.check_in_date >= '2024-11-01' AND r.check_out_date <= '2024-11-30';]]>
	</queryString>
